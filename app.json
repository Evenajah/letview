{
  "expo": {
    "name": "LetVision",
    "slug": "reactNativeTest",
    "privacy": "public",
    "sdkVersion": "33.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    /*"icon": "./assets/Logo.png",*/
    "splash": {
      "image": "./assets/logos.png",
      "resizeMode": "contain",
      "backgroundColor": "#2F4F4F"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.test.myapp"
    },
    "android": {
      "package": "com.test.myapp"
    }
  }
}